<div class="sf_admin_list scrollable_list no-more-tables">
   <table class="table table-striped table-condensed table-responsive table-hover">
      <thead>
         <tr>

            <th>{{ 'genericFields.name'|trans() }}</th>
            <th>{{ 'Duplicate.numberOfDuplicate'|trans() }}</th>
            <th id="sf_admin_list_th_actions">{{'actions.actions'|trans}}</th>
         </tr>
      </thead>
      <tbody>
         {% for duplicate in duplicates %}
            <tr>
               <td>{{ duplicate.object }}</td>
               <td>{{ duplicate.nb }}</td>
               <td>
                  <div class="btn-group">
                      <a href="{{ path('duplicate_details', {'id': duplicate.object.id, 'type': duplicateType }) }}" class="btn btn-default btn-xs "  title="{{'Duplicate.actions.cleanThisDuplicate'|trans()}}">
                         <i class="fa-solid fa-filter"></i>
                      </a>


                  </div>
               </td>
            </tr>
         {% endfor %}
      </tbody>
   </table>
</div>
