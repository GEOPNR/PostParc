{% extends 'base.html.twig' %}
{% block title %} {{'Lock.contentLocked'|trans()}} {% endblock %}
{% block body %}
  <div class="alert alert-warning" role="alert">
    
    <h1 class="alert-heading"><i class="fa-solid fa-lock" aria-hidden="true"></i> {{'Lock.contentLocked'|trans()}}</h1>
    <p>
      {{ 'Lock.contentMessage'|trans({'%content%': object, '%user%': app.user})|raw() }}
    </p>
    <hr>
    <a href="javascript:history.go(-1)" class="btn btn-default">{{ 'actions.backToPreviousPage'|trans() }}</a>
  </div>
{% endblock %}    