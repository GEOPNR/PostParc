{% if organizations|length >0 %}
<div class="alert alert-info">
    <button type="button" class="close" data-dismiss="alert">Ã—</button>
    <p><strong>{{'warning'|trans}} !</strong><br/>{{'Organization.flash.organizationExistingWithSameName'|trans}} :</p>
    <ul>
    {% for organization in organizations %}
         <li><a target="_new" href="{{ path('organization_show',{'id': organization.id}) }}">{{ organization }} {% if organization.entity.id != app.user.entity.id %} ({{ organization.entity }}  / <a href="mailto:{{ organization.createdBy.email }}">{{ organization.createdBy }}</a>){% if organization.isShared==0 %}  <span class="label label-warning">{{ 'notShared'|trans() }}</span> {% endif %}{% endif %}</a></li>
    {% endfor %}
    </ul>
</div>
{% endif %}
